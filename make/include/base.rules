.PHONY: report.main report.gcc report.modules report.var report.true

report.modules::

report.main:
	@$(MAKE) report.true REPORT=main

report.gcc:
	@$(MAKE) report.true REPORT=gcc

report.var:
	@$(MAKE) report.true REPORT=var

## needed for nested make (which drives each report)
report.true:
	@true
